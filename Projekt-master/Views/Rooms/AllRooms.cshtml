@model IEnumerable<Praktyki.Models.Room>

@{
    ViewData["Title"] = "Sale:";
}

<h1>Sale:</h1>

@if (Model.Count() > 0)
{
    @foreach (var item in Model)
    {
        <a asp-controller="Reservations" asp-action="RoomReservations" asp-route-id="@item.Id" class="btn btn-outline-dark btn-lg">@Html.DisplayFor(modelItem => item.Nr)</a>
    }
}
else
{
    <b class="mt-5">Brak dodanych sal.</b>
}